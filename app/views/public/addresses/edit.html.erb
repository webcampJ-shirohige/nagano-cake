<div class="container"> <div class="row mt-5"> <h2>配送先編集</h2> </div> <div class="row mt-3"> <table class=" table table-borderless"> <%= form_with model: @address, url: addresses_path do |f| %> <tr> <td><label for="address_post_code"><strong>郵便番号(ハイフンなし)</strong></label></td> <td><%= f.text_field :shipping_post_code, id:"address_post_code" %></td> </tr> <tr> <td><label for="address_address"><strong>住所</strong></label></td> <td><%= f.text_field :shipping_address, id:"address_address" %></td> </tr> <tr> <td><label for="address_name"><strong>宛名</strong></label></td> <td><%= f.text_field :shipping_name, id:"address_name" %></td> </tr> <tr> <td><%= f.submit "変更内容を保存", class: "btn btn-success" %></td> </tr> <% end %> </table> </div> </div>