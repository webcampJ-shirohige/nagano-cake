<h2>会員一覧</h2>
<table class='table'>
  <thead>
    <tr>
    <th>会員id</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @customers.each do |customer| %>
    <tr>
        <td><%= customer.id%></td>
        <td><%= link_to customer.last_name + customer.first_name, admin_customer_path(customer.id) %><td>
        <td><%= customer.email %></td>
        <td><%= customer.is_active %></td>
    </tr>
    <% end %>
　</tbody>